{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "noEmitOnError": false,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "lib": ["dom", "es2015", "es2017", "dom.iterable", "scripthost"]
  },
  "include": ["./src/**/*"]
}