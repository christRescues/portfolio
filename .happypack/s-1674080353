import { Link } from 'components/atoms';
import PropTypes from 'prop-types';
import React from 'react';
import styled from 'styled-components';

var Wrapper = styled.section.withConfig({
  displayName: 'AppList__Wrapper'
})(['display:flex;flex-wrap:wrap;justify-content:space-around;align-items:center;margin:100px 0;']);

var AppImage = styled.div.withConfig({
  displayName: 'AppList__AppImage'
})(['background:url(', ') no-repeat center;background-size:100% auto;margin:15px;width:250px;height:250px;border-radius:50%;transition:all 1s ease-in-out;&:hover{box-shadow:inset 0 0 0 110px rgba(255,255,255,0.4),inset 0 0 0 16px rgba(255,255,255,0.8),0 1px 2px rgba(0,0,0,0.1);}'], function (props) {
  return props.src;
});

var AppList = function AppList(_ref) {
  var apps = _ref.apps;

  var renderAppIcons = function renderAppIcons() {
    return apps.map(function (app) {
      return React.createElement(
        Link,
        { key: app.route, to: app.route },
        React.createElement(AppImage, { key: app.src, src: app.src })
      );
    });
  };
  return React.createElement(
    Wrapper,
    null,
    renderAppIcons()
  );
};

AppList.propTypes = {
  apps: PropTypes.array.isRequired
};

var _default = AppList;
export default _default;
;

