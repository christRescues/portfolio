import axios from 'axios';

// import proxyUrl from '../api'

export var FETCH_WIKIS = 'FETCH_WIKIS';

export function fetchWikis(term) {
  var url = 'https://en.wikipedia.org/w/api.php?action=query&list=search&srsearch=' + term + '&utf8=&format=json';
  // const reqUrl = `${proxyUrl}${url}`
  var reqUrl = url;

  var request = axios.get(reqUrl);
  return {
    type: FETCH_WIKIS,
    payload: request
  };
}
;

var _temp = function () {
  if (typeof __REACT_HOT_LOADER__ === 'undefined') {
    return;
  }

  __REACT_HOT_LOADER__.register(FETCH_WIKIS, 'FETCH_WIKIS', '/home/ryan/dev/port/src/store/wiki/action.js');

  __REACT_HOT_LOADER__.register(fetchWikis, 'fetchWikis', '/home/ryan/dev/port/src/store/wiki/action.js');
}();

;