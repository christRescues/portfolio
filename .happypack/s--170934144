  if (typeof __REACT_HOT_LOADER__ === 'undefined') {
    return;
  }

  __REACT_HOT_LOADER__.register(initialState, 'initialState', '/home/ryan/dev/port/src/store/quotes/reducer.js');

  __REACT_HOT_LOADER__.register(_default, 'default', '/home/ryan/dev/port/src/store/quotes/reducer.js');
